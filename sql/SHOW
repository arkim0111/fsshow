CREATE TABLE SHOW (
        SHOW_ID VARCHAR2(30) PRIMARY KEY, -- 페스티벌 ID
        SHOW_YEAR NUMBER(4) NOT NULL, -- 페스티벌 연도
        SHOW_NAME VARCHAR2(50) NOT NULL, -- 페스티벌 이름
        SHOW_ADDR VARCHAR2(100) , -- 페스티벌 장소
        SHOW_DATE VARCHAR2(20), -- 페스티벌 날짜
        SHOW_CELEB VARCHAR2(50), -- 페스티벌 출연진
        SHOW_POST_IMG VARCHAR2(1000), -- 페스티벌 포스터 사진
        SHOW_SEAT_IMG VARCHAR2(1000), -- 페스티벌 좌석배치도 사진
        SHOW_TICKET_DATE VARCHAR2(20), -- 페스티벌 티켓팅 날짜
        SHOW_TICKET_ADDR VARCHAR2(1000), -- 페스티벌 티켓팅 홈페이지
        SHOW_LEVEL NUMBER(1) NOT NULL --  페스티벌 요청 레벨
);
    CREATE SEQUENCE SHOW_IDX_SEQ
    START WITH 1;
COMMIT;